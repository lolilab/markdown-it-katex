{
  "name": "@andatoshiki/markdown-it-katex",
  "version": "0.0.3",
  "description": "⚒️ Mirror & fork of original deprecated markdown-it-katex package in favor of higher KaTeX version support for personal project usages.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "watch": "watchify browser.js -o bundle.js -v",
    "test": "uvu",
    "lint": "prettier --write .",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "publish-npm": "npm publish --access public --ignore-scripts --@andatoshiiki:registry='https://registry.npmjs.org'",
    "lint-commit": "git commit -m 'chore(lint): lint all files in project via prettier'",
    "minify": "uglifyjs index.js -c -m -o markdown-it-katex.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:lolilab/markdown-it-katex.git"
  },
  "keywords": [
    "markdown",
    "KaTeX",
    "math",
    "LaTeX",
    "markdown-it-plugin",
    "markdown-it"
  ],
  "author": "Anda Toshiki <hello@toshiki.dev>",
  "license": "MIT",
  "dependencies": {
    "katex": "^0.16.0"
  },
  "devDependencies": {
    "markdown-it": "^13.0.1",
    "markdown-it-testgen": "^0.1.6",
    "prettier": "2.8.4",
    "uglify-es": "^3.3.9",
    "uvu": "^0.5.3",
    "watchify": "4.0.0"
  }
}
